name = "riyapramodh - yes that is my name"
num = "123456"
print(type(name))
print(type(num))
print(name.isalnum())#string with no spaces and contains both letters and numbers
print(num.isalpha())#nstring with no spaces and has only letters
print(name.endswith("name"))
print(name.count("A"))#gives the no of times that letter has appeared
print(name.capitalize())
print(name.find("is"))
print(name.lower())
print(name.upper())
print(name.replace("riya", "kp"))
print(10*"this is python coding")
a = "10"
b = "20"
print(a + b)
print(int(a)+int(b))
print(10*str(int(a)+int(b)))
